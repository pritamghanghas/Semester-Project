#!/usr/bin/env python
PACKAGE = "dynamic_reconfigure_node"

from dynamic_reconfigure_node.parameter_generator_catkin import *

gen = ParameterGenerator()

gen.add("k_x", double_t, 0, "Position control gain",    6, 0, 100)
gen.add("str_param",    str_t,    0, "A string parameter",  "Hello World")
gen.add("bool_param",   bool_t,   0, "A Boolean parameter",  True)

exit(gen.generate(PACKAGE, "dynamic_reconfigure_node", "skyeControls"))
